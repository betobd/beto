                        <onclick class="script">clearTable(&quot;PersonnesACT&quot;);
// Get valeurs

  var /*Date*/ Debut = meibo.getFieldValue(&apos;DateDebut&apos;);
  var /*Date*/ Fin = meibo.getFieldValue(&apos;DateFin&apos;);


 //Format date xxxx-xx-xx
  
  var /*String*/ date = Debut.substring(0,4)+ &quot;-&quot; +Debut.substring(4,6)+ &quot;-&quot; +Debut.substring(6,8);
  var /*int*/ jour= parseInt(Debut.substring(6,8));
 
  // Filters
  
  var /*String*/ filter = &quot;&quot;;

   if( Debut != &quot;&quot; ) {
      if( filter != &quot;&quot; ) { 
           filter = filter + &quot; AND &quot;;
       }
      filter = filter + &quot;TCT_START_DATE  &gt;= &apos;&quot; + SQL_DATE_FORMAT.format(DATE_FORMAT.parse(Debut)) + &quot;&apos;&quot;;
     }

    if( Fin != &quot;&quot; ) {
         if( filter != &quot;&quot; ) { 
              filter = filter + &quot; AND &quot;; 
            }
         filter = filter + &quot;TCT_START_DATE  &lt;= &apos;&quot; + SQL_DATE_FORMAT.format(DATE_FORMAT.parse(Fin)) + &quot;&apos;&quot;;
    }
  
    
  meibo.getField(&quot;filter&quot;).setValue(filter);
  
  
  //Recherche et afichage table 1
  
  var /*ResultSet*/ resultSet = meibo.executeResource(&apos;Event&apos;, filter, 0, &apos;&apos;);
  var /*int*/ nEntries = resultSet.getEntryCount();
  if (nEntries != 0) 
  {   
      //meibo.showField(&quot;searchContainer&quot;, true);
      meibo.showField(&quot;error1&quot;, false);
    
      displaySearchA(&apos;Event&apos;, meibo.getFieldValue(&quot;attributeList&quot;));
  }
  
  else {
          meibo.showField(&quot;error1&quot;, true);
       }</onclick>
