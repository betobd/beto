                <onclick class="script">///$debug
var criteria = getFieldValue(&quot;entry&quot;);
try{
  var /*String*/ sqlQuery=&quot;select TAS_ID,TAS_NAME,TAS_ADDR1,TAS_STATE,TAS_TOWN,TCY_EN_GB_NAME,TCY_FR_FR_NAME from T_ADDRESS_TAS inner join T_COUNTRY_TCY on T_ADDRESS_TAS.TCY_ID = T_COUNTRY_TCY.TCY_ID&quot;;
  var /*String*/ filter = null;
  if(criteria.trim()!=&quot;&quot;){
    filter = &quot;TAS_NAME LIKE&apos;%&quot;+criteria+&quot;%&apos; OR TAS_STATE LIKE &apos;&quot;+criteria+&quot;%&apos; OR TAS_TOWN LIKE &apos;&quot;+criteria+&quot;%&apos; OR TCY_FR_FR_NAME LIKE &apos;&quot;+criteria+&quot;%&apos; OR TCY_EN_GB_NAME LIKE &apos;&quot;+criteria+&quot;%&apos;&quot;;
    sqlQuery = sqlQuery + &quot; where &quot; + filter;
  }
  //var results = meibo.executeResource(&quot;Address&quot;,filter,100,&quot;TAS_NAME,TAS_STATE,TAS_TOWN,TCY_EN_GB_NAME,TCY_FR_FR_NAME&quot;);
  // Note : ici executeResource ne rpermettrait pas la recherche sur le pays car il ne prend pas en compte les relations/attributs de relation
  var results = meibo.executeSqlRequest(&quot;TRAVEL_BCOM&quot;, sqlQuery, 0);
  if(results == null){
    throw &quot;Resultat de l&apos;execution de la requ&#234;te null&quot;;
  }
  setFieldValue(&quot;counter&quot;,&quot;&quot;+results.getEntryCount());
  //meibo.setPickerResultSet(&quot;People&quot;,results);
  if (results.getEntryCount() == 0){
    meibo.showField(&quot;results&quot;, false);
  }else{
    meibo.showField(&quot;results&quot;, true);
    meibo.clearTable(&quot;results&quot;);
    
    var /*Field*/resultsTable = meibo.getField(&quot;results&quot;);
    //resultsTable.fillInWithResultSet(results,0,0,&quot;
    // on ne peut pas utiliser fillInWithResultSet ici car il faut retourner TAS_ID en RowObject et avec fillInWithResultSet c&apos;est un DN qui serait retourn&#233;
    
    var line=0;
    for each(var entry in results){
        resultsTable.addRowObject(line, entry.TAS_ID[0]);
        //resultsTable.addRowObject(line, entry);
        resultsTable.addCell(line,0,entry.TAS_NAME[0]);
        resultsTable.addCell(line,1,entry.TAS_ADDR1[0]);
        resultsTable.addCell(line,2,entry.TAS_TOWN[0]);
        //TODO : use user language
        resultsTable.addCell(line,3,entry.TCY_FR_FR_NAME[0]);    
        line++;
    }
  }
}catch(e){
  meibo.logError(&quot;Erreur sur la recherche d&apos;adresse : &quot; + e);
}</onclick>
