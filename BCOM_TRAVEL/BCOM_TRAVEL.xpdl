  params.add(&quot;MAIL_SENDER&quot;, getMailParameter(&quot;MAIL_SENDER&quot;));
  params.add(&quot;targetPeople&quot;, targetPeopleArray);
  meibo.executePlugin(&quot;NotifyForReportValidation&quot;,params);

}
