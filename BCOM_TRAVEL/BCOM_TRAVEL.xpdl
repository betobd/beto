  if(eventAttendeesEntry!=null){
    meibo.logDebug(&quot;Suppression de l&apos;entr&#233;e &quot; + eventAttendeesEntry);
    //eventAttendeesRS.deleteEntry(0);
    meibo.deleteEntry(&quot;R_Attendees&quot;,eventAttendeesEntry.getDn());
