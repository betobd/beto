                <tabindex class="number">22</tabindex>
                <onclick class="script">///$debug
// build filter to launch the search

var currentUserID = meibo.getGlobalVar(GLOBALVAR_CURRENT_USER_ID);
// filtre de base : ne pas afficher les evenements priv&#233;s dont l&apos;utilisateur connect&#233; n&apos;est pas l&apos;auteur
// ni les &#233;venements pass&#233;s
var /*String*/ filter = &quot;(fkPRS_ID_AUTHOR=&quot; + currentUserID + &quot; OR TET_ISPRIVATE=0) AND TCT_START_DATE&gt;=&apos;&quot; + SQL_DATE_FORMAT.format(new java.util.Date()) + &quot;&apos;&quot;;

// read TET_NAME field
var /*String*/ TET_NAME = meibo.getFieldValue(&apos;TET_NAME&apos;);
if (TET_NAME.length != 0) {
  filter = filter + &quot; AND TET_NAME LIKE \&apos;%&quot; + TET_NAME + &quot;%\&apos;&quot;;
}
  
// read TET_COMMENT field
var /*String*/ TET_COMMENT = meibo.getFieldValue(&apos;TET_COMMENT&apos;);
if (TET_COMMENT.length != 0) {
  filter = filter + &quot; AND (TET_COMMENT LIKE \&apos;%&quot; + TET_COMMENT + &quot;%\&apos; OR TCT_COMMENT LIKE \&apos;%&quot; + TET_COMMENT + &quot;%\&apos;)&quot;;
}
  
// read fkTES_ID field
var /*String*/ fkTES_ID = meibo.getFieldValue(&apos;fkTES_ID&apos;);
if (fkTES_ID.trim().length != 0) {
  filter = filter + &quot; AND fkTES_ID = &quot; + fkTES_ID;
} 
  
// read fkTTY_ID field
var /*String*/ fkTTY_ID = meibo.getFieldValue(&apos;fkTTY_ID&apos;);
if (fkTTY_ID.trim().length != 0) {
  filter = filter + &quot; AND fkTTY_ID=&quot; + fkTTY_ID;
}
  

try{

  var results = executeResource(&quot;Event&quot;,filter,100,&quot;fkTCT_ID,fkTAS_ID,TET_NAME,TCT_START_DATE,TET_ISPRIVATE,TAS_TOWN&quot;);
  if(results == null){
    throw &quot;Resultat de l&apos;execution de la requ&#234;te null&quot;;
  }
  setFieldValue(&quot;counter&quot;,&quot;&quot;+results.getEntryCount());

  if (results.getEntryCount() == 0){
    meibo.showField(&quot;results&quot;, false);
  }else{
    meibo.showField(&quot;results&quot;, true);
    meibo.clearTable(&quot;results&quot;);
    
    var /*Field*/resultsTable = meibo.getField(&quot;results&quot;);
    //resultsTable.fillInWithResultSet(results,0,0,&quot;
    // on ne peut pas utiliser fillInWithResultSet ici car il faut retourner fkTCT_ID en RowObject et avec fillInWithResultSet c&apos;est un DN qui serait retourn&#233;
    // de plus ceci permet de formatter la date
    
    var line=0;
    for each(var entry in results){
        resultsTable.addRowObject(line, entry.fkTCT_ID[0]);
        resultsTable.addCell(line,0,entry.TET_NAME[0]);
        resultsTable.addCell(line,1,DATE_FORMAT.format(SQL_DATE_FORMAT.parse(entry.TCT_START_DATE[0])));
        resultsTable.addCell(line,2,entry.TAS_TOWN[0]);
        line++;
    }
  }
}catch(e){
  meibo.logError(&quot;Erreur sur la recherche d&apos;evenement avec le filtre &quot; + filter + &quot; : &quot; + e);
}</onclick>
