                <onclick class="script">var criteria = getFieldValue(&quot;entry&quot;);
var results = executeResource(&quot;TravelPeople&quot;,&quot;PRS_SIRNAME LIKE&apos;&quot;+criteria+&quot;%&apos; OR PRS_NAME LIKE &apos;&quot;+criteria+&quot;%&apos; OR PRS_LOGIN=&apos;&quot;+criteria+&quot;&apos;&quot;,100,&quot;PRS_SIRNAME,PRS_NAME,PRS_LOGIN&quot;);
setFieldValue(&quot;counter&quot;,&quot;&quot;+results.getEntryCount());
//meibo.setPickerResultSet(&quot;People&quot;,results);
if (results.getEntryCount() == 0){
  meibo.showField(&quot;results&quot;, false);
}else{
  meibo.showField(&quot;results&quot;, true);
  meibo.clearTable(&quot;results&quot;);
  
  var /*Field*/resultsTable = meibo.getField(&quot;results&quot;);
  //resultsTable.fillInWithResultSet(results,0,0,&quot;PRS_NAME,PRS_SIRNAME,PRS_LOGIN&quot;);
  // on ne peut pas utiliser fillInWithResultSet ici car il faut retourner PRS_ID en RowObject et avec fillInWithResultSet c&apos;est un DN qui serait retourn&#233;
  
  var line=0;
  for each(var entry in results){
      resultsTable.addRowObject(line, entry.PRS_ID[0]);
      //resultsTable.addRowObject(line, entry);
      resultsTable.addCell(line,0,entry.PRS_NAME[0]);
      resultsTable.addCell(line,1,entry.PRS_SIRNAME[0]);
      resultsTable.addCell(line,2,entry.PRS_LOGIN[0]);    
      line++;
  }
}</onclick>
